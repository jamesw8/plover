{
  "manifest_version": 2,

  "name": "Plover",
  
  "description": "Helps user stay on track while browsing the internet",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["popup.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "storage"
  ]
}
